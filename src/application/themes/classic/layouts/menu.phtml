?>
</ul>
</div>
<?php
function createUl($val)
{
	if( !empty($val['CHILD']) )
	{
		echo "<ul>";
		  foreach($val['CHILD'] as $value )
		  {
			$class=null;
			if( !empty($value['CHILD']) )
			 $class='class="parent"';
			echo '<li><a href="'.$value['URL'].'" '.$class.'><span>'.$value['RESOURCE']."</span></a>";
			createUl($value);
			echo "</li>";
		  }
		echo "</ul>";
	}  
}
?>