if($_SESSION['menu']['objmenu']){
	foreach($_SESSION['menu']['objmenu'] as $val)
	{ 
		echo "<li><a href='".$val['URL']."'><span>".$val['RESOURCE']."</span></a>";
		createUl($val);	
		echo "</li>";
	}
}else{
echo '<div style="color:#fff;padding-top:10px;font-weight:bold;">No menu selected for this user role.</div>';
